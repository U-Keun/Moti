{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-capability",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "log:default",
    {
        "identifier": "shell:allow-spawn",
        "allow": [
            {
                "name": "binaries/HandDaemon",
                "sidecar": true,
                "args": false
            }
        ]
    }
  ]
}
